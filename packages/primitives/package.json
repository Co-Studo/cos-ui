{
  "name": "@cos-ui/primitives",
  "version": "0.4.1",
  "description": "Headless UI components for React",
  "homepage": "https://github.com/Co-Studo/cos-ui/tree/main/packages/primitives#readme",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cosui-primitives.umd.cjs",
  "module": "./dist/cosui-primitives.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cosui-primitives.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cosui-primitives.umd.cjs"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "watch": "vite build --watch"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "styled-components": "^5.3.6"
  }
}
