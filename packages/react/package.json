{
  "name": "@cos-ui/react",
  "version": "0.2.0",
  "description": "Styled components for CoStudo",
  "homepage": "https://github.com/Co-Studo/cos-ui/tree/main/packages/react#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc-alias && babel --root-mode upward --extensions .ts,.tsx src --out-dir dist",
    "watch": "tsc && tsc-alias && babel --root-mode upward --watch --extensions .ts,.tsx src --out-dir dist",
    "playground": "yarn workspace @cos-ui/playground-react dev",
    "clean": "rimraf ./dist"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.3.6",
    "styled-reset": "^4.4.2"
  },
  "dependencies": {
    "@cos-ui/primitives": "^0.2.0"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  }
}
