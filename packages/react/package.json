{
  "name": "@cos-ui/react",
  "version": "0.4.0",
  "description": "Styled components for CoStudo",
  "homepage": "https://github.com/Co-Studo/cos-ui/tree/main/packages/react#readme",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cosui-react.umd.cjs",
  "module": "./dist/cosui-react.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cosui-react.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cosui-react.umd.cjs"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "styled-reset": "^4.4.5",
    "@cos-ui/primitives": "^0.4.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "styled-components": "^5.3.6"
  }
}
